{
  "name": "Change to something",
  "description": "テキストエリアに入力したら特定の処理を行い出力する拡張機能",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.2.0.min.js"]
  }]
}